{
  "name": "awraris",
  "version": "1.0.0",
  "description": "`Awraris` a simple cli music streaming app.",
  "bin": {
    "awraris": "dist/cli.js"
  },
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/danielddemissie/awraris.git"
  },
  "scripts": {
    "build": "bun build src/cli.ts --compile --outfile dist/cli.js --target node20 --format cjs --minify",
    "clean": "rm -rf dist",
    "commit": "cz",
    "commitlint": "commitlint --edit",
    "compile": "tsc",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "keywords": [
    "typescript",
    "starter",
    "cli",
    "bootstrap"
  ],
  "author": "Daniel Demelash <danielddemissie@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/blessed": "^0.1.25",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/prompts": "^2.4.9",
    "@types/signale": "^1.4.7",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.1.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "consola": "^3.2.3",
    "dotenv": "^16.4.5",
    "ora": "^8.2.0",
    "yargs": "^18.0.0",
    "yt-stream": "^1.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
